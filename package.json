{
    "name": "safedonate",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start:client": "npm run dev --prefix client",
        "start:server": "npm start --prefix server",
        "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "install:all": "npm install --prefix client && npm install --prefix server"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    }
}
